<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/jokes_nav_path"
    app:startDestination="@id/jokesFetchAndStore">

    <fragment
        android:id="@+id/jokesFetchAndStore"
        android:name="com.example.jokesapp.presentation.sceens.joke_fetch_and_store.JokesFetchAndStore"
        android:label="JokesFetchAndStore"
        tools:layout="@layout/fragment_joke_fetch_and_store">
        <action
            android:id="@+id/action_jokesFetchAndStore_to_jokesListFragment"
            app:destination="@id/jokesListFragment" />
    </fragment>

    <fragment
        android:id="@+id/jokesListFragment"
        android:name="com.example.jokesapp.presentation.sceens.jokes_list.JokesListFragment"
        android:label="JokesListFragment"
        tools:layout="@layout/fragment_jokes_list" />
</navigation>